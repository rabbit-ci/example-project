{
  "repo": "https://github.com/rabbit-ci/example-project.git",
  "steps": [
    {
      "name": "`cat` the README file.",
      "provider": "docker",
      "before_script": "apt-get update\napt-get install -y git",
      "boxes": [
        "ubuntu:vivid"
      ],
      "script": "git rev-parse HEAD\ncat README.md\n"
    },
    {
      "name": "Show Ubuntu version",
      "provider": "virtualbox",
      "before_script": "sudo apt-get update\nsudo apt-get install -y git",
      "boxes": [
        "ubuntu/vivid64"
      ],
      "script": "git rev-parse HEAD\nlsb_release -a\necho \"Did it work?\"\n"
    }
  ]
}
